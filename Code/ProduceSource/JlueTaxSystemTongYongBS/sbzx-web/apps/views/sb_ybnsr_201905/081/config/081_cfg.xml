    <Constrain>
        <Page from="1" to="-1">
            <Row from="7" to="12">
                <Rule against-warn="第page页第row行本期实际抵减税额应大于等于0且小于等于本期应抵减税额。" DeductRow="6">
                    <items>
                        <item>7</item>
                        <item>8</item>
                    </items>
                    <cell-formula>(value("#1")&gt;=value("#2")) and (value("#2")&gt;=0)</cell-formula>
                </Rule>
                <Rule AreaCode="4403" against-warn="第page页第row栏减免性质代码及名称不能修改。" DeductRow="6">
                    <items>
                        <item>1</item>
                        <item>3</item>
                        <item>10</item>
                    </items>
                    <cell-formula>((value("#1")=value("#3")) and (value("#2") = 1)) or (value("#2")&lt;&gt;1)
                    </cell-formula>
                </Rule>
            </Row>
            <Row from="19" to="25">
                <Rule against-warn="第page页第row栏免税项目免税额应大于等于0。" DeductRow="9">
                    <items>
                        <item>9</item>
                    </items>
                    <cell-formula>value("#1")&gt;=0</cell-formula>
                </Rule>
                <Rule AreaCode="4403," against-warn="第page页第row栏第1列免征增值税项目销售额应大于等于0。" DeductRow="9">
                    <items>
                        <item>5</item>
                    </items>
                    <cell-formula>value("#1")&gt;=0</cell-formula>
                </Rule>
                <Rule AreaCode="4403,"
                      against-warn="第page页第row栏:第1列免征增值税项目销售额大于0时，第5列免税额应大于0;第1列免征增值税项目销售额等于0时，第5列免税额应等于0。"
                      DeductRow="9">
                    <items>
                        <item>5</item>
                        <item>9</item>
                    </items>
                    <cell-formula>((value("#1")&gt;0) and (value("#2")&gt;0)) or ((value("#1")=0) and (value("#2")=0))
                        or ((value("#1")&lt;0) or (value("#2")&lt;0))
                    </cell-formula>
                </Rule>
                <Rule AreaCode="4403," against-warn="第page页第row栏:第5列免税额不能大于第1列免征增值税项目销售额*17%。" DeductRow="9">
                    <items>
                        <item>5</item>
                        <item>9</item>
                    </items>
                    <cell-formula>round(value("#2"),2)&lt;=round(value("#1")*0.17,2)</cell-formula>
                </Rule>
                <Rule against-warn="第page页第row栏第4列免税销售额对应的进项税额应小于等于第3列扣除后免税销售额。" DeductRow="9">
                    <items>
                        <item>7</item>
                        <item>8</item>
                    </items>
                    <cell-formula>value("#1")&gt;=value("#2")</cell-formula>
                </Rule>
                <Rule AreaCode="36,"
                      against-warn="第page页二、免税项目部分第row栏：第5列免税额 必须小于等于第3栏扣除后免税销售额*17%-第4列免税销售额对应的进项税额并且第5列免税额大于等于第3列扣除后免税销售额*6%-第4列免税销售额对应的进项税额，或者 第5列免税额等于第3栏扣除后免税销售额*3%并且第4列免税销售额对应的进项税额等于0。"
                      DeductRow="9">
                    <items>
                        <item>7</item>
                        <item>8</item>
                        <item>9</item>
                    </items>
                    <cell-formula>((value("#3")&gt;=value("#1")*0.06-value("#2")-0.01) and (value("#3")&lt;=value("#1")*0.17-value("#2")+0.01))
                        or ((value("#3")=round(value("#1")*0.03,2)) and (value("#2")=0))
                    </cell-formula>
                </Rule>
                <Rule AreaCode="36," against-warn="第page页二、免税项目部分第row栏：第3列“扣除后免税销售额”＞0，请填写免税销售额对应的进项税额。" DeductRow="9">
                    <items>
                        <item>7</item>
                        <item>8</item>
                        <item>2</item>
                        <item>9</item>
                    </items>
                    <cell-formula>(not ((value("#1")&gt;0) and (value("#2")=0))) or ((value("#3")=0001092303) or
                        (value("#3")=0001092308)) or (value("#1")*0.03=value("#4"))
                    </cell-formula>
                </Rule>
            </Row>
        </Page>
    </Constrain>
