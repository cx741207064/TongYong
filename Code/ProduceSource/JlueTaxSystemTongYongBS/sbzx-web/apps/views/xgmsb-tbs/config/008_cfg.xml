      </Row>
      <Row from="19" to="25">
        <Rule against-warn="第page页第row行免税项目免税额应大于等于0。" DeductRow="17">
          <items>
            <item>9</item>
          </items>
          <cell-formula>value("#1")&gt;=0</cell-formula>
        </Rule>
        <Rule AreaCode="4403," against-warn="第page页第row栏第1列免征增值税项目销售额应大于等于0。" DeductRow="9">
          <items>
            <item>5</item>
          </items>
          <cell-formula>value("#1")&gt;=0</cell-formula>
        </Rule>
        <Rule AreaCode="4403," against-warn="第page页第row栏:第1列免征增值税项目销售额大于0时，第5列免税额应大于0;第1列免征增值税项目销售额等于0时，第5列免税额应等于0。" DeductRow="9">
          <items>
            <item>5</item>
            <item>9</item>
          </items>
          <cell-formula>((value("#1")&gt;0) and (value("#2")&gt;0)) or ((value("#1")=0) and (value("#2")=0)) or ((value("#1")&lt;0) or (value("#2")&lt;0))</cell-formula>
        </Rule>
        <Rule AreaCode="4403," against-warn="第page页第row栏:第5列免税额不能大于第1列免征增值税项目销售额*17%。" DeductRow="9">
          <items>
            <item>5</item>
            <item>9</item>
          </items>
          <cell-formula>round(value("#2"),2)&lt;=round(value("#1")*0.17,2)</cell-formula>
        </Rule>
        <Rule AreaCode="36," against-warn="第page页二、免税项目部分第row行：减免税申报明细表第5栏免税额必须＝第1栏免征增值税项目销售额*3% 或者减免税申报明细表第5栏免税额必须＝免征增值税项目销售额*5%。" DeductRow="9">
          <items>
            <item>5</item>
            <item>9</item>
          </items>
          <cell-formula>((value("#2")=round(value("#1")*0.03+0.000001,2)) or (value("#2")=round(value("#1")*0.05+0.000001,2)))</cell-formula>
        </Rule>
